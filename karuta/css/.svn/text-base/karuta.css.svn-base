/* -------------------------------------------------- */
/* Container */

body {
	font-size: 12px;
}

#content .texte{
	text-align:justify;
}

/* -------------------------------------------------- */
/* Container */

#container {
	padding-top: 80px;
}

textarea {
	width:100%;
	height:200px;
}

/* -------------------------------------------------- */
/* Main Nav */

.sidebar-title {
	font-size: 18px;
	font-weight: bold;
	line-height: 22px;
	margin-bottom: 15px;
}
#main-nav .title a {
	color: #333;
}

#main-nav div.divider {
	background-color: #e5e5e5;
	border-bottom: 1px solid #fff;
	height: 1px;
	margin-bottom: 10px;
	width: 100%;
}

#main-nav .nav-list {
	padding: 0;
}

#main-nav .nav-list .branch-top > a {
	background-image: url('images/main-nav-arrow-closed.png');
	background-position: 0 9px;
	background-repeat: no-repeat;
	font-weight: bold;
	margin-left: 0;
	margin-right: 0;
	padding-right: 10px;
}
#main-nav .nav-list .selected > a {
	background-image: url('images/main-nav-arrow-opened.png');
}

#main-nav .nav-list ul.nested li {
	margin-bottom: 5px;
}
#main-nav .nav-list ul.nested {
	display: none;
	padding-top: 11px;
}



#sidebar li.selected ul.nested {
	display: block;
}
#sidebar li.selected ul.nested li.selected a {
	background-image: none;
	color: #000;
}
#sidebar li.nested {
	text-indent: 15px;
}
#sidebar .number {
	color: #aaa;
	float: right;
	font-size: 11px;
	font-weight: normal;
}


.activeTreeColor, .parentTreeColor {
	color:#01A9DB;
}

.noTreeColor {
	color:inherit;
}

#wait-window,
#saved-window {
	width:100px;
	margin-left:-50px;
	background-color:lightgrey;
	position:fixed;
	top:25%;
	left:50%;
}



/* -------------------------------------------------- */
/* Content */


hr {
	margin-bottom: 7px;
	margin-top: 5px;
}



#content h1,
#content h2,
#content h3,
#content h4,
#content h5,
#content h6 {
	margin-top: 0;
}

#content hr.spacer {
	margin-bottom: 66px;
}



#content .icon-move {
	cursor: move;
}

#content .comment {
	font-size: 12px;
	padding-bottom: 22px;
	padding-left: 40px;
}

#content h2 .popover-inner {
	font-size: 13px;
	font-weight: normal;
	line-height: 16px;
}

