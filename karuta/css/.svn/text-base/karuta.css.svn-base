	margin-top:10px;
}

.asmRoot .title {
	font-size: 24px;
	font-weight: bold;
	line-height: 30px;
}


.asmStructure .title {
	font-size: 24px;
	font-weight: bold;
	line-height: 30px;
}

.asmStructure .asmStructure .title,
.asmRoot .asmStructure .title {
	font-size: 12px;
	font-weight: normal;
	line-height: 20px;
}

.asmUnit .title {
	font-size: 24px;
	font-weight: bold;
	line-height: 30px;
}

.asmStructure .asmUnit .title,
.asmRoot .asmUnit .title {
	font-size: 12px;
	font-weight: normal;
	line-height: 20px;
}

.asmUnitStructure .title {
	font-size: 20px;
	font-weight: bold;
	line-height: 20px;
}

.asmUnitStructure .asmUnitStructure .title {
	font-size: 16px;
	font-weight: bold;
	line-height: 20px;
}

.asmUnitStructure .asmUnitStructure .asmUnitStructure .title {
	font-size: 14px;
	font-weight: bold;
	line-height: 20px;
}

.asmUnitStructure .asmUnitStructure .asmUnitStructure .asmUnitStructure .title {
	font-size: 12px;
	font-weight: bold;
	line-height: 20px;
}

.asmUnitStructure .asmUnitStructure .asmUnitStructure .asmUnitStructure .asmContext .title,
.asmUnitStructure .asmUnitStructure .asmUnitStructure .asmContext .title,
.asmUnitStructure .asmUnitStructure .asmContext .title,
.asmUnitStructure .asmContext .title,
.asmContext .title {
	font-size: 12px;
	font-weight: normal;
	line-height: 16px;
}

